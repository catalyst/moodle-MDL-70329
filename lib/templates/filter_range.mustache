{{!
    @template core/filter_range

    Template for the form containing one or more filter rows.

    Example context (json):
    {
    }
}}
<select class="custom-select mb-1 mb-md-0 mr-md-2" data-filterfield="range" id="core-filter-rangetype-{{uniqid}}"
        aria-label="{{#str}}filtersetmatchdescription{{/str}}">
    <option value="0">{{#str}}after, question{{/str}}</option>
    <option value="1">{{#str}}before, question{{/str}}</option>
    <option value="2" selected>{{#str}}between, question{{/str}}</option>
</select>
