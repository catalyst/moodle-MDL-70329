{{!
    @template core_admin/setting_qbanksetting.mustache

    Admin question bank setting template.

    Context variables required for this template:
    * name - plugin name

    Example context (json):
    {
        "name": "createcourse",
    }
}}

{{#str}}qbanksortdescription, question{{/str}}
<br>
<br>
<div class="list list-group" id="sortable">
    {{#names}}
        <div class="item list-group-item list-group-item-action">
            <span class="retrievable">
            {{>core/drag_handle}}
                {{name}}
            </span>
        </div>
    {{/names}}
</div>
<br>
{{#str}}qbankgotomanageqbanks, question{{/str}} <a href="{{{manageqbankurl}}}">{{#str}}manageqbanks, admin{{/str}}</a>
